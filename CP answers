#include<stdio.h>
int main(){
double num,i;
printf("number: ");
scanf("%f", &num);
int fact =1;
for(i=1;i=<num;i++){
fact*=i;}
printf("factorial: %f",fact);
return 0;}

#include<stdio.h>
int main(){ char str[50]; 
int i=0, count =0;
printf("string: ");
scanf("%s", str);
while(str[i]!='\0'){
if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U')count++;
i++;
printf("number of vowels:%d ", count);
return 0;}

#include<stdio.h>
int main(){
int n, arr[50], i=0, largestnum;
printf("number of elements in array: ");
scanf("%d", &n);
for(i=0;i<n;i++){
printf("arr[%d]= ", i);
scanf("%d". &arr[i]);}
largestnum=arr[0];
for(i=0;i<n;i++){
if(largestnum<arr[i]) largestnum=arr[i];
}
printf("the maximum number in given array is: %d",largestnum);
return 0;}

#include<stdio.h>
int main(){
int n, arr[50], i=0, smallestnum;
printf("number of elements in array: ");
scanf("%d", &n);
for(i=0;i<n;i++){
printf("arr[%d]= ", i);
scanf("%d", &arr[i]);}
smallestnum=arr[0];
for(i=0;i<n;i++){
if(smallestnum > arr[i]) smallestnum=arr[i];
}
printf("the maximum number in given array is: %d",smallestnum);
return 0;}

#include<stdio.h>
int main(){
int a,b;
printf("number1: ");
scanf("%d", &a);
printf("\nnumber2: ");
scanf("%d",&b);
a=a+b;
b=a-b;
a=a-b;
printf("\nafter swapping number1:%d", a);
printf("\nafter swapping number2: %d",b);
return 0;}

#include <stdio.h>
int main() {
   int year;
   printf("Enter a year: ");
   scanf("%d", &year);
   if (year % 400 == 0) {
      printf("%d is a leap year.", year);
   }
   else if (year % 100 == 0) {
      printf("%d is not a leap year.", year);
   }
   else if (year % 4 == 0) {
      printf("%d is a leap year.", year);
   }
   else {
      printf("%d is not a leap year.", year);
   }
return 0;}

